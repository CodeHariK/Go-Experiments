{
  "database": {
    "host": "host",
    "port": 5432,
    "user": "username",
    "password": "password",
    "dbname": "defaultdb",
    "max_connections": 100,
    "timeout": 30,
    "ssl_mode": "require"
  },
  "discord": {
    "client_id": "your_client_id",
    "client_secret": "your_client_secret",
    "scopes": [
      "identify",
      "email"
    ],
    "redirect_uri": "http://localhost:3000/auth/discord/callback",
    "auth_url": "https://discord.com/api/oauth2/authorize",
    "token_url": "https://discord.com/api/oauth2/token"
  },
  "server": {
    "port": 3000,
    "log_level": "debug",
    "enable_metrics": true
  },
  "feature_flags": {
    "new_feature": true,
    "beta_access": false
  },
  "session": {
    "max_age": 2592000,
    "http_only": true,
    "secure": false
  }
}